<!doctype html> 
<html lang="en"> 
<head> 
  <meta charset="utf-8"> 
 
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> 
  <title>Are we first yet?</title> 
  <link rel="stylesheet" href="/style.css"> 
  <link rel="shortcut icon" href="/favicon.png"> 
</head> 
<body> 
<div id="container"> 
  <h1>Are We First Yet?</h1>
  <div id="arewefirstyet"></div> 
  <h2><%= first %></h2> 
  <table>
    <% for (var idx in current_rankings) { %>
      <tr><th><%= idx %></th><td><%= current_rankings[idx] %></td></tr>
    <% } %>
  <a href='http://promotejs.com/' title='Promote JS! A Worldwide Call for Improving JS Documentation Visibility'><img src='http://static.jsconf.us/promotejsvs.png' height='280' width='160' alt='Promote JS!'/></a> 

<footer>
  <p>Created by the <a href="http://jsconf.us">Pirates of JSConf</a>.</p>
</footer>
</div> 
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script> 
<script src="./ext/flot/jquery.flot.js"></script> 
<script> 
 
function getDate(days) {
  return (new Date()).getTime() - days * 3600 * 24 * 1000;
}
 
var series = <%- data %>;
var options = {
       series: {
           lines: { show: true },
           points: { show: true }
       },
       legend: { noColumns: 2 },
       xaxis: { 
         mode: "time", 
         timeformat: "%y/%m/%d",
         minTickSize: [1, "day"]
       }
   };
 
$(function () {
  $.plot($("#arewefirstyet"), series, options);
});
</script> 
</body> 
</html>